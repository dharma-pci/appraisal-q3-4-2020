<odoo>
    <record id="delivery_order_wizard_line_view_form" model="ir.ui.view">
        <field name="name">delivery.order.wizard.line.view.form</field>
        <field name="model">delivery.order.wizard.line</field>
        <field name="arch" type="xml">
            <form create="false" edit="false">
                <sheet>
                    <group>
                        <field name="product_id"/>
                        <field name="name"/>
                        <field name="product_uom_qty"/>
                    </group>

                    <group>
                        <field name="company_id"/>
                        <field name="price_unit"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>